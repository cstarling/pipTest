                                     - C/Lfw is the second list entry
                                     - Ls/Rs is the third list entry
                                */								
      sequence<unsigned short> lstAudioPairIDs;   // reuse from DCM_Dgio.idl
      boolean MetadataPresent; // do we have metadata for this Audio program
    };

