    {
      enuTrackTypeV3  enTrackType;
      octet           oComponentTag;
      octet           oStreamType;     ///< Valid if stream type tracking.
      string          strLanguage;     ///< Valid if stream type tracking and audio, teletext or subtitles match.
      unsigned long   ulDescriptorTag; /**< Valid if stream type tracking.
                                        *   - 0x56 to match teletext.
                                        *   - 0x59 to match subtitles.
                                        */
      unsigned short  usInputPid;
      unsigned long   ulOrder;         ///< Reserved for future use. Must be zero when set and ignored when read.
      unsigned short  usParentEsOutputPid;
      unsigned short  usCasId;
      unsigned long   ulFilterMask;    /**< Bit 0: Loose stream type matching (valid if stream type tracking):
                                        *          If set, this criterion will not only match streams with an
                                        *          exactly matching stream type, but all audio, video or
                                        *          data streams depending on which category the specified
                                        *          stream type belongs to.
                                        *   Bit 1: Loose language matching (valid if stream type tracking):
                                        *          If set and this criterion matches audio, subtitles or
                                        *          teletext streams, this criterion also matches streams with
                                        *          another language than strLanguage.
                                        *   All other bits must be zero when set and ignored when read.
                                        */
      string          strRegistrationDescriptor; ///< Optional registration descriptor requirement for stream type tracking
    };


