    /**
     * Defines an audio pair present in the incoming SDI signal of the
     * specified port.
     */
    struct DGIO_SDI_AudioPair_t
    {
      IPS_RefTS_t            RefTS; ///< Only 1 TS per SDI port; Member 'Ref' = TS index = PortNr
      unsigned short         AudioPairID; ///< A value between 0 and 7 (included), referencing 1 of the audio pairs in the SDI signal.
      DGIO_SDI_AudioFormat_t DGIO_SDI_AudioFormat;
      boolean                bMetaDataPresent;   
    };
    typedef sequence<DGIO_SDI_AudioPair_t> DGIO_SDI_AudioPairList_t;

    /**
     * Defines an Audio Component by specifying its PID (and others
     * like language and the expected audio format) and associates it
     * to 1./ a board, port, TS, service on one hand and to 2./ SDI Audio
     * Pair(s) on the other hand.
     *
     * Multiple of these Audio Components can be instantiated, even if
     * no incoming SDI signal is currently present. Thus allowing for
     * pre-configuration and provisioning.
     */
    struct DGIO_SDI_InES_AudioComponent_t
    {
      IPS_RefSvc_t             Service;
      unsigned short           PID; ///< PID of this ES.

                               /** Values between 0 and 7 (included), referencing the audio pairs in the SDI signal.
                                   Possible list items are:
                                     - 1 item: The pair for stereo.
                                     - 3 items: All pairs are PCM formatted audio. (= 5.1 Surround)
                                   When Surround, then:
                                     - L/R is the first list entry
                                     - C/Lfw is the second list entry
                                     - Ls/Rs is the third list entry
                                */
      sequence<unsigned short> lstAudioPairIDs;
      DGIO_SDI_AudioFormat_t   ExpectedSDI_AudioFormat; ///< Use 0xFF (see member 'CompressedFormat') when any incoming compressed audio format is allowed.
      boolean                  bHasLanguageSettings;

                               /** Possible AudioType octets are:
                                *    0x00 Undefined
                                *    0x01 Clean effects
                                *    0x02 Hearing impaired
                                *    0x03 Visual impaired commentary
                                */
      IPS_LanguageSettings_t   Language;
      unsigned long            PID_InProperty; ///< Value is always identical to member 'IPS_In_ESSettings_t.PID_Property'.
    };
    typedef sequence<DGIO_SDI_InES_AudioComponent_t> DGIO_SDI_InES_AudioComponentList_t;

